  },
  "homepage": "https://github.com/andrewjamesford/andrewford-co-nz-11ty#readme",
  "devDependencies": {
    "@11ty/eleventy": "2.0.0",
    "@11ty/eleventy-img": "^2.0.1",
    "@11ty/eleventy-plugin-rss": "1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "4.2.0",
