{
  "name": "andrewford-co-nz-11ty",
  "version": "1.0.0",
  "description": "andrewford.co.nz blog in 11ty",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npx @11ty/eleventy --serve",
    "debug": "DEBUG=Eleventy* npx @11ty/eleventy",
    "build": "npx @11ty/eleventy",
    "article": "bash article.sh",
    "clear": "rm -rf _site",
    "social": "npx itdk view resoc-templates/default/resoc.manifest.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrewjamesford/andrewford-co-nz-11ty.git"
  },
  "keywords": [
    "11ty",
    "blog"
  ],
  "author": "Andrew Ford",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andrewjamesford/andrewford-co-nz-11ty/issues"
  },
  "homepage": "https://github.com/andrewjamesford/andrewford-co-nz-11ty#readme",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.2",
    "@11ty/eleventy-img": "^2.0.1",
    "@11ty/eleventy-plugin-rss": "^1.0.9",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.1.0",
    "@resoc/eleventy-plugin-social-image": "^0.0.4",
    "@resoc/netlify-plugin-social-image": "^0.0.9",
    "eleventy-plugin-page-assets": "^0.3.0",
    "prettier": "2.5.0"
